    "@material/layout-grid": "^0.24.0",
    "@material/linear-progress": "^0.25.0",
    "@material/list": "^0.26.0",
    "@material/menu": "^0.25.0",
    "@material/ripple": "^0.26.0",
    "@material/theme": "^0.25.0",
    "@material/toolbar": "^0.25.0",
    "@material/typography": "^0.3.0",
    "classnames": "^2.2.5",
    "lodash": "^4.17.4",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
