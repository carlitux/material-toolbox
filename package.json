{
  "name": "material-toolbox",
  "version": "0.1.0",
  "description": "React Material Design Components for Web",
  "repository": "https://github.com/carlitux/material-toolbox",
  "author": "Luis Carlos Cruz Carballo",
  "license": "MIT",
  "flow": "flow",
  "prepublish": "npm run prepublish",
  "watch": {
    "build-dev": "src/**/*.{js,scss}"
  },
  "scripts": {
    "build-dev": "babel src --out-dir . --copy-files",
    "build": "NODE_ENV=production babel src --out-dir . --copy-files",
    "watch": "npm-watch",
    "prepublish": "npm run build",
    "clean": "shx rm -rf lib/",
    "lint": "eslint .",
    "test": "jest",
    "test:watch": "jest --watchAll"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-display-name": "^6.25.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-config-airbnb-base": "^12.0.1",
    "eslint-config-prettier": "^2.6.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "flow-bin": "^0.56.0",
    "flow-typed": "^2.1.5",
    "npm-run-all": "^4.1.1",
    "npm-watch": "^0.3.0",
    "prettier": "^1.7.3",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-transform-catch-errors": "^1.0.2",
    "tern": "^0.21.0"
  },
  "dependencies": {
    "@material/drawer": "^0.5.9",
    "@material/layout-grid": "^0.4.4",
    "@material/list": "^0.2.20",
    "@material/theme": "^0.4.0",
    "@material/toolbar": "^0.4.11",
    "@material/typography": "^0.3.0",
    "classnames": "^2.2.5",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  }
}
